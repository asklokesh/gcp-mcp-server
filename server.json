{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.asklokesh/gcp-mcp-server",
  "description": "MCP server for Google Cloud Platform API integration",
  "title": "Gcp",
  "repository": {
    "url": "https://github.com/asklokesh/gcp-mcp-server",
    "source": "github"
  },
  "version": "0.1.1",
  "packages": [
    {
      "registryType": "pypi",
      "registryBaseUrl": "https://pypi.org",
      "identifier": "gcp-mcp-server",
      "version": "0.1.1",
      "runtimeHint": "uvx",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "name": "GCP_PROJECT_ID",
          "description": "GCP Project ID",
          "isRequired": true
        },
        {
          "name": "GOOGLE_APPLICATION_CREDENTIALS",
          "description": "Path to GCP service account key JSON file",
          "isRequired": true
        }
      ]
    }
  ]
}